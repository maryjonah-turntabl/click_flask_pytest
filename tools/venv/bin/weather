#!/home/mary/Desktop/PREPARATION_MORGAN_STANLEY/ms_click_project/tools/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'weather-cli','console_scripts','weather'
__requires__ = 'weather-cli'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('weather-cli', 'console_scripts', 'weather')()
    )
